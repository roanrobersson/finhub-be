{
	"tabWidth": 2,
	"printWidth": 80,
	"useTabs": true,
	"singleQuote": false,
	"jsxSingleQuote": false,
	"trailingComma": "none",
	"bracketSpacing": true,
	"arrowParens": "always",
	"endOfLine": "auto",
	"plugins": ["@ianvs/prettier-plugin-sort-imports"],
	"importOrder": ["^@nestjs", "<THIRD_PARTY_MODULES>", "", "^[./]", ""],
	"importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy"],
	"importOrderTypeScriptVersion": "5.0.0"
}
